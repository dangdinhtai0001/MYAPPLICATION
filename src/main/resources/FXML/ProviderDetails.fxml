<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
      minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/10.0.1"
      fx:controller="controller.ProviderDetailsController">
    <children>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="THÔNG TIN NHÀ CUNG CẤP" underline="true">
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </VBox.margin>
        </Text>
        <VBox>
            <children>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Thông tin cơ bản" underline="true">
                    <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </VBox.margin>
                </Text>
                <HBox>
                    <children>
                        <HBox fx:id="hBoxName" alignment="CENTER_LEFT">
                            <children>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Tên đầy đủ">
                                    <HBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="20.0" top="5.0"/>
                                    </HBox.margin>
                                </Text>
                                <TextField fx:id="txtName">
                                    <HBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                    </HBox.margin>
                                </TextField>
                                <ImageView fx:id="validateName" fitHeight="25.0" fitWidth="25.0" pickOnBounds="true"
                                           preserveRatio="true">
                                    <HBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                    </HBox.margin>
                                </ImageView>
                            </children>
                            <HBox.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </HBox.margin>
                        </HBox>
                        <HBox fx:id="hBoxShortName" alignment="CENTER_LEFT" layoutX="10.0" layoutY="10.0">
                            <children>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Mã nhà cung cấp">
                                    <HBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                    </HBox.margin>
                                </Text>
                                <TextField fx:id="txtShortName">
                                    <HBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                    </HBox.margin>
                                </TextField>
                                <ImageView fx:id="validateShortName" fitHeight="25.0" fitWidth="25.0"
                                           pickOnBounds="true" preserveRatio="true">
                                    <HBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                    </HBox.margin>
                                </ImageView>
                            </children>
                            <HBox.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </HBox.margin>
                        </HBox>
                    </children>
                    <VBox.margin>
                        <Insets bottom="10.0" left="30.0" right="10.0" top="10.0"/>
                    </VBox.margin>
                </HBox>
                <Text layoutX="10.0" layoutY="23.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Thông tin liên lạc"
                      underline="true">
                    <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </VBox.margin>
                </Text>
                <HBox layoutX="10.0" layoutY="27.0">
                    <children>
                        <VBox>
                            <children>
                                <HBox fx:id="hBoxAddress" alignment="CENTER_LEFT">
                                    <children>
                                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Đia Chỉ">
                                            <HBox.margin>
                                                <Insets bottom="5.0" left="5.0" right="40.0" top="5.0"/>
                                            </HBox.margin>
                                        </Text>
                                        <TextField fx:id="txtAddress">
                                            <HBox.margin>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                            </HBox.margin>
                                        </TextField>
                                        <ImageView fx:id="validateAddress" fitHeight="25.0" fitWidth="25.0"
                                                   pickOnBounds="true" preserveRatio="true">
                                            <HBox.margin>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                            </HBox.margin>
                                        </ImageView>
                                    </children>
                                    <VBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                    </VBox.margin>
                                </HBox>
                                <HBox fx:id="hBoxPhone" alignment="CENTER_LEFT">
                                    <children>
                                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Số điện thoại">
                                            <HBox.margin>
                                                <Insets bottom="5.0" left="5.0" right="8.0" top="5.0"/>
                                            </HBox.margin>
                                        </Text>
                                        <TextField fx:id="txtPhone">
                                            <HBox.margin>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                            </HBox.margin>
                                        </TextField>
                                        <ImageView fx:id="validatePhone" fitHeight="25.0" fitWidth="25.0"
                                                   pickOnBounds="true" preserveRatio="true">
                                            <HBox.margin>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                            </HBox.margin>
                                        </ImageView>
                                    </children>
                                    <VBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                    </VBox.margin>
                                </HBox>
                                <HBox fx:id="hBoxMail" alignment="CENTER_LEFT" layoutX="10.0" layoutY="35.0">
                                    <children>
                                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="E-mail">
                                            <HBox.margin>
                                                <Insets bottom="5.0" left="5.0" right="44.0" top="5.0"/>
                                            </HBox.margin>
                                        </Text>
                                        <TextField fx:id="txtMail">
                                            <HBox.margin>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                            </HBox.margin>
                                        </TextField>
                                        <ImageView fx:id="validateMail" fitHeight="25.0" fitWidth="25.0"
                                                   pickOnBounds="true" preserveRatio="true">
                                            <HBox.margin>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                            </HBox.margin>
                                        </ImageView>
                                    </children>
                                    <VBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                    </VBox.margin>
                                </HBox>
                                <HBox fx:id="hBoxFax" alignment="CENTER_LEFT" layoutX="10.0" layoutY="60.0">
                                    <children>
                                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Fax">
                                            <HBox.margin>
                                                <Insets bottom="5.0" left="5.0" right="59.0" top="5.0"/>
                                            </HBox.margin>
                                        </Text>
                                        <TextField fx:id="txtFax">
                                            <HBox.margin>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                            </HBox.margin>
                                        </TextField>
                                        <ImageView fx:id="validateFax" fitHeight="25.0" fitWidth="25.0"
                                                   pickOnBounds="true" preserveRatio="true">
                                            <HBox.margin>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                            </HBox.margin>
                                        </ImageView>
                                    </children>
                                    <VBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                    </VBox.margin>
                                </HBox>
                            </children>
                            <HBox.margin>
                                <Insets/>
                            </HBox.margin>
                        </VBox>
                        <VBox layoutX="10.0" layoutY="10.0">
                            <children>
                                <HBox fx:id="hBoxStatus" alignment="CENTER_LEFT">
                                    <children>
                                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Tình Trạng">
                                            <HBox.margin>
                                                <Insets bottom="5.0" left="5.0" right="25.0" top="5.0"/>
                                            </HBox.margin>
                                        </Text>
                                        <ComboBox fx:id="comboBoxStatus" prefWidth="170.0"/>
                                        <ImageView fx:id="validateStatus" fitHeight="25.0" fitWidth="25.0"
                                                   pickOnBounds="true" preserveRatio="true">
                                            <HBox.margin>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                            </HBox.margin>
                                        </ImageView>
                                    </children>
                                    <VBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                    </VBox.margin>
                                </HBox>
                                <HBox fx:id="hBoxBankAccount" alignment="CENTER_LEFT">
                                    <children>
                                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Số Tài khoản">
                                            <HBox.margin>
                                                <Insets bottom="5.0" left="5.0" right="10.0" top="5.0"/>
                                            </HBox.margin>
                                        </Text>
                                        <TextField fx:id="txtBankAccount" prefHeight="25.0" prefWidth="168.0">
                                            <HBox.margin>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                            </HBox.margin>
                                        </TextField>
                                        <ImageView fx:id="validateBankAccount" fitHeight="25.0" fitWidth="25.0"
                                                   pickOnBounds="true" preserveRatio="true">
                                            <HBox.margin>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                            </HBox.margin>
                                        </ImageView>
                                    </children>
                                    <VBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                    </VBox.margin>
                                </HBox>
                                <HBox fx:id="hBoxDescription" alignment="CENTER">
                                    <children>
                                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Ghi chú">
                                            <HBox.margin>
                                                <Insets bottom="5.0" left="5.0" right="35.0" top="5.0"/>
                                            </HBox.margin>
                                        </Text>
                                        <TextArea fx:id="txtDescription" maxHeight="75.0" maxWidth="170.0">
                                            <HBox.margin>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                            </HBox.margin>
                                        </TextArea>
                                        <ImageView fx:id="validateDescription" fitHeight="25.0" fitWidth="25.0"
                                                   pickOnBounds="true" preserveRatio="true">
                                            <HBox.margin>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                            </HBox.margin>
                                        </ImageView>
                                    </children>
                                    <VBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                    </VBox.margin>
                                </HBox>
                            </children>
                        </VBox>
                    </children>
                    <VBox.margin>
                        <Insets bottom="10.0" left="30.0" right="10.0" top="10.0"/>
                    </VBox.margin>
                </HBox>
            </children>
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </VBox.margin>
        </VBox>
        <ButtonBar prefHeight="40.0" prefWidth="200.0">
            <buttons>
                <Button mnemonicParsing="false" onAction="#submit" text="Lưu ">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../Icon/Save.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button layoutX="445.0" layoutY="16.0" mnemonicParsing="false" onAction="#cancel" text="Hủy">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../Icon/Cancel.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
            </buttons>
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </VBox.margin>
        </ButtonBar>
    </children>
</VBox>
